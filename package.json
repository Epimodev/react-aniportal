{
  "name": "react-aniportal",
  "version": "0.1.6",
  "description": "A react component which render a portal with enter/leave animations",
  "author": "Frederic Godin",
  "repository": "Epimodev/react-aniportal",
  "license": "MIT",
  "main": "module/index.js",
  "module": "module/index.js",
  "typings": "module/index.d.ts",
  "keywords": [
    "react",
    "portal",
    "animations",
    "typescript",
    "tslint"
  ],
  "files": [
    "src",
    "module"
  ],
  "scripts": {
    "build": "rm -rf ./module && tsc",
    "lint": "tslint -c tslint.json 'src/**/*.ts?(x)'",
    "check-format": "prettier --list-different 'src/**/*.ts?(x)'",
    "storybook": "start-storybook --ci -s ./ -p 9001 -c .storybook",
    "prepare": "yarn lint && yarn check-format && yarn build"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@storybook/react": "^5.1.8",
    "@types/classnames": "^2.2.8",
    "@types/lodash": "^4.14.134",
    "@types/react": "^16.8.20",
    "@types/react-dom": "^16.8.4",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.0.6",
    "husky": "^2.4.1",
    "prettier": "^1.18.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "ts-loader": "^6.0.2",
    "tslint": "^5.17.0",
    "tslint-config-airbnb": "^5.11.1",
    "typescript": "^3.5.2"
  },
  "peer-dependencies": {
    "react": "^16.0.0",
    "react-dom": "^16.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint && yarn check-format"
    }
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "tslib": "^1.10.0"
  }
}
